<div class="w-full">
<%= render "notice"%>

<h1 class="font-bold text-xl mb-6 pb-2 border-b-2">
  <%= t('.home_title') %>
</h1>
<%= turbo_stream_from "products" %>
  <ul id="products-list">

      <% @products.each do |product| %>

          <%= render partial: "product", object: product if product.title.present? %>

      <% end %>

  </ul>
</div>
